{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "warning"
  },
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [            
      {
       
        "type": "field",
        "ip": ["geoip:cn"],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "domain": [
          "geoSite:cn"
        ],
        "outboundTag": "block" 
      },
      {
        "type": "field",
        "domain": [
          "geosite:category-ads-all" 
        ],
        "outboundTag": "block"
      }
    ]
   },
  "inbounds": [
    # ios QX专用
    {
        "port": 16789,
        "protocol": "vless",
        "settings": {
          "clients": [
            {
              "id": "b70189db-726e-4f18-8ad4-3f9506733686", 
              "flow": "xtls-rprx-vision"
            }
          ],
          "decryption": "none",
          "fallbacks": [
            {
              "dest": 1310,
              "xver": 1
            }
          ]
        },
        "streamSettings": {
          "network": "tcp",
          "security": "tls",
          "tlsSettings": {
            "alpn": [
                "http/1.1"
            ],
            "certificates": [
              {
                "certificateFile": "/usr/local/etc/xray/cert.pem",
                "keyFile": "/usr/local/etc/xray/key.pem"
              }
            ]
          }
        },
        "sniffing": {
          "enabled": true,
          "destOverride": ["http","tls"]
        }
    },
    # windows专用 
    {
      "port": 16256,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "87792db3-b519-412d-a2a9-131787df6d9e", 
            "flow": "xtls-rprx-vision"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "alpn": [
              "http/1.1"
          ],
          "certificates": [
            {
              "certificateFile": "/usr/local/etc/xray/cert.pem",
              "keyFile": "/usr/local/etc/xray/key.pem"
            }
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http","tls"]
      }
    },
    # 回落到trojan
    {
            "port": 1310,
            "listen": "127.0.0.1",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "3cb5d4a3", 
                        "level": 0,
                        "email": "love@example.com"
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true
                }
            }
    }
    
  ],
  
  "outbounds": [
    
    {
      "tag": "direct",
      "protocol": "freedom"
    },
    #黑洞 阻断所有流量
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {
          "response": {
            "type": "none"
           }
       }
    }
  ]
}
